{
	"name": "landing",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"install:git-hooks": "lefthook add pre-commit && lefthook add pre-push && lefthook add commit-msg",
		"uninstall:git-hooks": "lefthook uninstall",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.21.0",
		"@types/nodemailer": "6.4.8",
		"@typescript-eslint/eslint-plugin": "6.0.0",
		"@typescript-eslint/parser": "6.0.0",
		"eslint": "8.44.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.32.2",
		"eslint-plugin-unused-imports": "3.0.0",
		"lefthook": "^1.4.1",
		"postcss": "8.4.24",
		"prettier": "3.0.0",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "4.0.1",
		"svelte-check": "3.4.6",
		"svelte-preprocess-import-assets": "^1.0.0",
		"tailwindcss": "3.3.2",
		"tslib": "2.6.0",
		"typescript": "5.1.6",
		"vite": "4.3.9"
	},
	"dependencies": {
		"@sentry/sveltekit": "7.58.1",
		"kudu-components": "0.0.6",
		"kudu-ui-system": "0.1.17",
		"kudu-utils": "^0.0.5",
		"nodemailer": "6.9.3",
		"svelte-email": "0.0.4"
	},
	"type": "module"
}
