{
	"name": "landing",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"install:git-hooks": "lefthook add pre-commit && lefthook add pre-push && lefthook add commit-msg",
		"uninstall:git-hooks": "lefthook uninstall",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.20.4",
		"@types/nodemailer": "6.4.8",
		"@typescript-eslint/eslint-plugin": "5.59.11",
		"@typescript-eslint/parser": "5.59.11",
		"autoprefixer": "10.4.14",
		"eslint": "8.43.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.30.0",
		"eslint-plugin-unused-imports": "2.0.0",
		"lefthook": "^1.4.1",
		"postcss": "8.4.24",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "4.0.1",
		"svelte-check": "3.4.3",
		"svelte-preprocess-import-assets": "^1.0.0",
		"tailwindcss": "3.3.2",
		"tslib": "2.6.0",
		"typescript": "5.1.6",
		"vite": "4.3.9"
	},
	"dependencies": {
		"@sentry/sveltekit": "7.55.2",
		"kudu-ui-system": "0.0.1",
		"nodemailer": "6.9.3",
		"svelte-email": "0.0.4",
		"svelte-icons": "2.1.0"
	},
	"type": "module"
}
